{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "es6": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 2016
  },
  "rules": {
    "strict": 0,
    "arrow-parens": 0,
    "import/no-unresolved": 0,
    "max-len": 0,
    "no-underscore-dangle": 0,
    "no-continue": 0,
    "newline-per-chained-call": 0,
    "no-multiple-empty-lines": 0,
    "eol-last": 0,
    "no-plusplus": 0,
    "one-var": 0,
    "one-var-declaration-per-line": 0,
    "import/prefer-default-export": 0
  }
}
